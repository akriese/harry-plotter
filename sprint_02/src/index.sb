      // TODO: change visibility if necessary
      //.attr("r", i => makeRadius(V[i]) / currentZoom) // scale radius by the current zoom
  }

  //svg.call(zoom);
  const update = () => accidentDots
    .attr("cx", i => map.latLngToLayerPoint([P[i][1], P[i][0]]).x)
    .attr("cy", i => map.latLngToLayerPoint([P[i][1], P[i][0]]).y)
  if (map !== undefined) map.on("zoom", update)
