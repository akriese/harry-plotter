function LeafletAccidentMap(data, {
  topo,
  topoCoordSystem = "EPSG:32633",
  divName = "mother",
  minZoom = 11,
  maxZoom = 19,
  states,
  stateTitle,
  tooltipContent,
  dotCoords,
}) {
  // get topo bounds and convert them to WGS84
  var bounds = topojson.bbox(topo)
  var boundsArr = [bounds.slice(0, 2), bounds.slice(2, 4)]
  var projBounds = [proj4(topoCoordSystem, "WGS84", boundsArr[0]).reverse(),
                    proj4(topoCoordSystem, "WGS84", boundsArr[1]).reverse()]

  // draw map
  var map = L.map(divName).fitBounds(projBounds);
  L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
    minZoom: minZoom,
    maxZoom: maxZoom,
  }).addTo(map);

  // LOR polygons
  for (f of states.features) {
    var polygon = L.polygon(f.geometry.coordinates[0]).bindTooltip("LOR").addTo(map);
  }

  // accidentMarkers
  const markersCluster = L.markerClusterGroup({ showCoverageOnHover: false });

  data.forEach(d => {
    markersCluster.addLayer(L.marker(dotCoords(d).reverse()))
  })

  map.addLayer(markersCluster);

  return map;

  // Create dots with d3
  /*
  L.svg({clickable:true}).addTo(map)
  var [dots, P] = AccidentMap(data, {
    borders: statemesh,
    getCoordinates: d => ([d.XGCSWGS84, d.YGCSWGS84]),
    projection: d3.geoMercator(),
    topo: berlin_lor_topo,
    width: 1500,
    height: 1000,
    map: map
    //overlay: d3.select(map.getPanes().overlayPane).select('svg').attr("pointer-events", "auto")
  })
  */
}
